neuron validExpressionTest:
  state:
    Resist Ohm = 8Ohm 
    Ampere A = 3A 

    V1 (kg*m*m)/(A*s**3) = 10 V  #verbose Type
    #V2 (kg*m*m)/(A*s**(2+1)) = 10 V   #TODO: this should work
    W1 MW = 10 [kJ/ms] #magnitude   
     
    hertz Hz = 7 [s**-1] 
    newton N = 10kg* 1m / 1 s**2
    pascal Pa =  7 * newton / 2m**2
  end

  function foo(amp A, ohm Ohm) V:  #Unit type in functions
    return amp * ohm
  end

  input:
      spikeInh   <- inhibitory spike
      spikeExc   <- excitatory spike
      currents <- current
  end

  parameters:
    V2 V = foo(7 A,2 Ohm) #function call    
  end

  output: spike

  internals:
    S_1 ms   = resolution() #predefined Functions
  end

  update:
    Ampere = 7A #Assignment

    if Resist > 3Ohm: # Comparison
     #NOOP
    elif 8A >= Ampere:  #Comparison
      emit_spike()
    end
  end

end
